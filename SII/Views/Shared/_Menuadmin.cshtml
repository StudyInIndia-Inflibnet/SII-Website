                            if ((Session["Phase"].ToString() == "Phase-1"))
                            {

                                <li>
                                    <a href="@Url.Content("~/")Admin/MockRound/Index"><i class="icon icon mdi mdi-gradient"></i><span>1st Counselling </span></a>
                                </li>
                                <li>
                                    <a href="@Url.Content("~/")Admin/MockRoundSecond/Index"><i class="icon mdi mdi-filter-list"></i><span>2nd Counselling</span></a>
                                </li>
                                <li>
                                    <a href="@Url.Content("~/")Admin/Reports/Index"><i class="icon mdi mdi-filter-list"></i><span>Counselling Reports (Phase-1)</span></a>
                                </li>
                            }

                            if ((Session["Phase"].ToString() == "Phase-2"))
                            {
                                <li>
                                    <a href="@Url.Content("~/")Admin/Phase2/Reports"><i class="icon mdi mdi-filter-list"></i><span>Report (Phase-2)</span></a>
                                </li>

                            }
                            if ((Session["Phase"].ToString() == "Phase-3"))

                                <a href="@Url.Content("~/")Admin/Upload"><i class="icon mdi mdi-file"></i><span>Upload Documents</span></a>
                            </li>
                        }
                        if (Session["is_doc_verification"].ToString().ToLower() == "true")
                        {
                            <li class="parent @(ViewBag.CurrentMenu == "VerifyDocuments" ? "open":"")">
                                <a href="#"><i class="icon mdi mdi-file-document-box"></i><span>Document Verification</span></a>

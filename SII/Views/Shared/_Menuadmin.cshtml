                                <ul class="sub-menu">
                                    <li>
                                        <a href="@Url.Content("~/")Admin/DisciplineMaster?id=niche">Discipline</a>
                                    </li>
                                    <li>
                                        <a href="@Url.Content("~/")Admin/ProgrammeLevelMaster?id=niche">Programme Level</a>

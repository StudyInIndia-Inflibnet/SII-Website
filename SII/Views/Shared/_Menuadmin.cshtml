
<div class="left-sidebar-wrapper">
    <a href="#" class="left-sidebar-toggle">Dashboard</a>
    <div class="left-sidebar-spacer">
        <div class="left-sidebar-scroll">
            <div class="left-sidebar-content">
                <ul class="sidebar-elements">
                    @if (Session["AdminRole"].ToString() == "Super Admin" || Session["AdminRole"].ToString() == "Admin" || Session["AdminRole"].ToString() == "Niche Course" || Session["AdminRole"].ToString() == "Call Center")
                    {



                        <li class="divider">Menu</li>
                        <li class="active">
                            <a href="@Url.Content("~/")Admin/ParticipationYears"><i class="icon mdi mdi-home Super-Admin"></i><span>Dashboard</span></a>
                        </li>

                        if ((Session["AdminRole"].ToString() == "Super Admin" || Session["AdminRole"].ToString() == "Admin"))
                        {
                            <li>
                                <a href="@Url.Content("~/")Admin/Agency"><i class="icon mdi mdi-home Super-Admin"></i><span>Agency</span></a>

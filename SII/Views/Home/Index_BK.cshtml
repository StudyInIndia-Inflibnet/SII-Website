        <div class="row wow fadeInUp owl-carousel" style="visibility: visible; animation-name: fadeInUp;">
            @{
                SIIRepository.Adminservice.BlogPost_Repository _objRepo = new SIIRepository.Adminservice.BlogPost_Repository();
                SIIModel.Admin.mBlogPost _obj = new SIIModel.Admin.mBlogPost();
                System.Data.DataSet _ds = _objRepo.SELECT_BLOGPOST(_obj);
                if (_ds != null)
                {
                    if (_ds.Tables[0].Rows.Count > 0)
                    {
                        foreach (System.Data.DataRow _dr in _ds.Tables[0].Rows)
                        {
                            <div class="col-sm-12 wow fadeInRight">
                                <div class="single-blog">
                                    <div class="blog-img"> <img src="@Url.Content("~/")@_dr["BlogImage"].ToString()" alt=""> <span>@(Convert.ToDateTime(_dr["CreatedDate"].ToString()).ToString("dd-MMM-yyyy | hh:mm tt"))</span> </div>
                                    <div class="blog-box">
                                        <h4>@_dr["BlogTitle"].ToString()</h4>
                                        <span class="blog-btn"><a href="@Url.Content("~/")blog/Page/@_dr["UniqueCode"].ToString()" class="blog-readmore">Read more</a></span>
                                    </div>
                                </div>
                            </div>
                        }
                    }
                }
            }
        </div>
        <div class="about-carousel-left">
            <div class="about-carousel-left-slide">
                <div class="about-carousel-left-single-slide">
                    <div class="row wow fadeInUp" style="visibility: visible; animation-name: fadeInUp;">

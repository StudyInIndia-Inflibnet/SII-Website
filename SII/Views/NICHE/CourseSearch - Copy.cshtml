            float: left;
            padding: 15px;
        }

            .panel-details .institute-name {
                margin-bottom: 5px;
                font-size: 20px;
                font-weight: bold;
            }

            .panel-details p, .panel-buttons p {
                margin-bottom: 0px;
            }

        .panel-buttons {
            padding: 15px 10px;
            border-left: 1px solid #969696;
            width: 25%;
            float: left;
            height: 100%;
        }

            .panel-buttons .institute-rattings i {
                font-size: 20px;
                margin-right: 5px;
                color: #efb726;
            }

            .panel-buttons .total-fees {
                margin-top: 15px;
                margin-bottom: 10px;
            }

            .panel-buttons .total-fees-label, panel-buttons .total-fees-value {
                vertical-align: middle;
                height: 100%;
            }

            .panel-buttons .total-fees-label {
                padding-right: 0;
            }

            .panel-buttons .total-fees-value {
                padding-left: 0;
            }

            .panel-buttons .total-fees-value {
                font-size: 35px;
                font-weight: bold;
            }

                .panel-buttons .total-fees-value small {
                    font-size: 50%;
                    font-weight: bold;
                }

            .panel-buttons .btn-more-details {
                background-color: #5e5e5e;
                border-color: #5e5e5e;
                color: #fff;
            }

            .panel-buttons .btn-apply-now {
                background-color: #f18a37;
                border-color: #f18a37;
                color: #fff;
            }
    </style>
}
@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $('#myBtn').remove();

            page_init();
        });
        function page_init() {
            var filterState = [];
            var strfilterProgramLevel = "@ViewBag.Discipline";
            var filterProgramLevel = strfilterProgramLevel.split(',');
            var filterNatureOfCourse = [];
            var filterBranch = [];
            var filterInstituteType = [];
            FillInstitutes(filterState, filterProgramLevel, filterNatureOfCourse, filterBranch, filterInstituteType);
        }

        function FillInstitutes(filterState, filterProgramLevel, filterNatureOfCourse, filterBranch, filterInstituteType, token = $('input[name="__RequestVerificationToken"]', $('#frm')).val()) {
            $.ajax({
                method: 'POST',
                url: $('#hdfBaseUrl').val() + 'NICHE/SearchCourseList',
                data: { __RequestVerificationToken: token, State: filterState.join(','), ProgramLevel: filterProgramLevel.join(','), NatureOfCourse: filterNatureOfCourse.join(','), BranchID: filterBranch.join(','), InstituteType: filterInstituteType.join(','), Type: 'InstituteWise' },
                async: false,
                success: function (data) {
                }
            }).done(function (data) {
                $('.loading').hide();
            }).error(function () {
                $('.loading').hide();
            });
        }
    </script>
}
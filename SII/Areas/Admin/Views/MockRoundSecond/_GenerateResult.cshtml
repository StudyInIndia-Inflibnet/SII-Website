

@{
    ViewBag.Title = "Course";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<style>
    p {
        margin: 0 0 0px !important;
    }
</style>
@{
    SIIModel.Admin.MockRound _obj = new SIIModel.Admin.MockRound();
    SIIRepository.Adminservice.MockRoundRepository _objRepository = new SIIRepository.Adminservice.MockRoundRepository();
    _obj.Type = "Count";

    {
        System.Data.DataSet _ds = _objRepository.GenerateResult_Second("GenerateResult", "Generate", @TempData["ProgramlevelId"].ToString());
        if (_ds != null)
        {

            if (_ds.Tables[0].Rows.Count > 0)
            {
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-default panel-border-color panel-border-color-custom panel-table">
                            <div class="panel-heading">
                                <div class="tools">
                                    <a href="@Url.Content("~/")Admin/MockRound" class="btn btn-success">Back</a>
                                </div>
                            </div>
                            <div class="tab-container">
                                <div class="tab-content">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="panel-body table-responsive" style="height:50%">
                                                <h3  class="text-success"> Mock result for <b class="text-danger">@TempData["ProgramleveName"].ToString() </b>  program level has been generated successfully...!!</h3>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
            else
            { <p class="text-danger">Somthing wrong.</p>
            }
        }
        else
        {  <p class="text-danger">Somthing wrong.</p>}
    }
}
<!--Form Modals-->





@{
    ViewBag.Title = "Course";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<style>
    p {
        margin: 0 0 0px !important;
    }
</style>
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default panel-border-color panel-border-color-custom panel-table">
            <div class="panel-heading">
                @{string msglabel = "";

                    if (TempData["ReportFor"].ToString() == "")
                    {
                        msglabel = "All";
                    }
                    if (TempData["ReportFor"].ToString() == "1")
                    {
                        msglabel = "Allocatted Students";
                    }
                    if (TempData["ReportFor"].ToString() == "0")
                    {
                        msglabel = "De-Allocatted Students";
                    }
                }



                Merit List - @msglabel - <b class="text-primary">(@TempData["ProgramleveName"].ToString() || @TempData["Discipline"].ToString())</b>
                <div class="tools">
                    <a href="@Url.Content("~/")Admin/MockRound" class="btn btn-success">Back</a>
                </div>
            </div>
            <div class="tab-container">
                <div class="tab-content">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="panel-body table-responsive">
                                <table class="table table-bordered table-hover" id="tbl">
                                    <thead>
                                        <tr>
                                            <th>Sr</th>
                                            <th>Student Id</th>
                                            <th>Student Name</th>
                                            <th>Country</th>
                                            <th>Email</th>
                                            <th>Mobile</th>
                                            <th>Merit Marks</th>
                                            <th>Merit Rank</th>
                                            <th>Filled Choices</th>
                                            <th>Seat Status</th>
                                            <th>Acadamic Info.</th>
                                            <th>Institute Action</th>
                                            <th>Accept/Reject Remarks</th>
                                        </tr>
                                    </thead>
                                    <tbody id="tbodyCostofliving">
                                        @{
                                            SIIModel.Admin.MockRound _obj = new SIIModel.Admin.MockRound();
                                            SIIRepository.Adminservice.MockRoundRepository _objRepository = new SIIRepository.Adminservice.MockRoundRepository();
                                            _obj.Type = "Count";
                                            System.Data.DataSet _ds = _objRepository.GenerateResult_Second("Merit", "MeritRank", @TempData["ProgramlevelId"].ToString(), @TempData["Discipline_Id"].ToString(), null, @TempData["ReportFor"].ToString() , @TempData["InstituteAction"].ToString() );
                                            if (_ds != null)
                                            {
                                                if (_ds.Tables[0].Rows.Count > 0)
                                                {
                                                    foreach (System.Data.DataRow _dr in _ds.Tables[0].Rows)
                                                    {
                                                        <tr>
                                                            <td>@_dr["RowNo"].ToString()</td>
                                                            <td>@_dr["Student_ID"].ToString()</td>
                                                            <td> @_dr["FirstName"].ToString() @_dr["MiddleName"].ToString() @_dr["LastName"].ToString()</td>
                                                            <td>@_dr["Country_Name"].ToString()</td>
                                                            <td>@_dr["Email"].ToString()</td>
                                                            <td> @_dr["Mobile"].ToString()</td>
                                                            <td>@_dr["MeritMarks"].ToString()</td>
                                                            <td>@_dr["MeritRank"].ToString()</td>
                                                            <td><a target="_blank" href="@Url.Content("~/")Admin/MockroundSecond/ViewDetails?For=StudentFillChoiceDisciplineWise&&PrgId=@_dr["ProgramLevel_ID"].ToString()&&Prgname=@TempData["ProgramleveName"].ToString()&&Discipline_Id=@TempData["Discipline_Id"].ToString()&&Discipline=@TempData["Discipline"].ToString()&&StudentId=@_dr["Student_ID"].ToString()">@_dr["FilledChoices"].ToString()</a></td>
                                                            <td><span class=@_dr["StatusClass"].ToString()>@_dr["Status"].ToString()</span></td>
                                                            <td><a target="_blank" href="@Url.Content("~/")Admin/MockroundSecond/ViewAcadamic?For=AcademicInformation&&ReportFor=AcademicInformation&&StudentId=@_dr["Student_ID"].ToString()&&StudentName=@_dr["FirstName"].ToString() @_dr["LastName"].ToString()"><Button class="btn btn-space btn-default"><i class="icon mdi mdi-desktop-mac"></i></Button></a></td>
                                                            <td>@_dr["InstituteStatus"].ToString()</td>
                                                            <td>@_dr["SeatApproveReason"].ToString()</td>
                                                        </tr>
                                                    }
                                                }
                                                else
                                                {
                                                    <tr>
                                                        <td colspan="6">
                                                            No data found.
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                            else
                                            {
                                                <tr>
                                                    <td colspan="6">
                                                        No data found.
                                                    </td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section styles{
    <link rel="stylesheet" type="text/css" href="/assets/lib/datatables/css/dataTables.bootstrap.min.css" />
    <link rel="stylesheet" href="/assets/css/style.css" type="text/css" />
}
@section scripts{
    <script src="/assets/lib/jquery/jquery.min.js" type="text/javascript"></script>
    <script src="/assets/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>
    <script src="/assets/js/main.js" type="text/javascript"></script>
    <script src="/assets/lib/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/js/jquery.dataTables.min.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/js/dataTables.bootstrap.min.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/dataTables.buttons.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.html5.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.flash.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.print.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.colVis.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.bootstrap.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#mainToggle").addClass("be-wrapper be-collapsible-sidebar be-collapsible-sidebar-collapsed");
            //$('#tbl').DataTable({
            //    dom: 'Bfrtip',
            //    buttons: ['excel']
            //});

            $("#tbl").DataTable({
                searching: true,
                paging: true,
                info: true,                   // TO DISPLAY THE INFO 'SHOWING 1 TO X OF Y ENTRIES'
                lengthMenu: [[25, 50, -1], [25, 50, "All"]],
                dom: 'Bfrtip',
                buttons: [
            {
                extend: 'csvHtml5',
                text: 'Export',
                titleAttr: 'CSV'
            },
                ],
                columnDefs: [{
                    "targets": 5,
                    "orderable": false
                }]
            });


        });
    </script>
}


@{
    ViewBag.Title = "Student Fill Choice List";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<style>
    p {
        margin: 0 0 0px !important;
    }
</style>
<div class="row">
   
    <div class="col-md-12">
        <div class="panel panel-default panel-border-color panel-border-color-custom">
            <div class="tab-container">
                <div class="tab-content">
                    <div class="tools" style="float:right">
                        <a href="@Url.Content("~/")Admin/Reports" class="btn btn-success">Back</a>
                    </div>
                    @*Select Institute*@

                    <div class="row">
                        <div class="col-md-12">
                            <form id="frmStep2" method="post">
                                <h4>
                                    @{
                                        SIIRepository.Adminservice.ReportsRepository _2objRepository = new SIIRepository.Adminservice.ReportsRepository();
                                        System.Data.DataSet _2ds = _2objRepository.Report("AllChoice", "AllChoice");
                                        if (_2ds != null)
                                        {
                                            if (_2ds.Tables[0].Rows.Count > 0)
                                            {
                                                <p> Country Wise Student Choice Filling  <b class="text-danger"> ( @_2ds.Tables[0].Rows.Count  - Total Country)</b> </p>
                                            }
                                        }}
                                </h4>
                                <hr />
                                <div class="table-responsive">
                                    <table class="table table-striped table-hover table-bordered table-fw-widget choice-table data-grid2" id="tbl1">
                                        <thead>
                                            <tr>
                                                <th width="5%">#</th>
                                                <th width="30%">Student Name</th>
                                                <th width="30%">Student Id</th>
                                                <th width="30%">Choice Seq. #</th>
                                                <th width="30%">Institute</th>
                                                <th width="10%">Prg. Level</th>
                                                <th width="10%">Discipline</th>
                                                <th>Nature Of Course</th>
                                                <th>Branch</th>
                                                <th width="10%">Fee Waiver</th>
                                                <th width="10%">Remaks</th>
                                                <th width="30%">Eligibility Criteria</th>
                                                <th>Qualification Req.</th>
                                                <th>JEE(Main)</th>
                                                <th>JEE(Adv)</th>
                                                <th>IELTS</th>
                                                <th>GMAT</th>
                                                <th>TOFEL</th>
                                                <th>SAT</th>
                                            </tr>
                                        </thead>
                                        <tbody id="tbodyFill">
                                            @{
                                                SIIRepository.Adminservice.ReportsRepository _1objRepository = new SIIRepository.Adminservice.ReportsRepository();
                                                System.Data.DataSet _1ds = _1objRepository.Report("AllChoice", "AllChoice");
                                                if (_1ds != null)
                                                {
                                                    if (_1ds.Tables[0].Rows.Count > 0)
                                                    {
                                                        foreach (System.Data.DataRow _dr in _1ds.Tables[0].Rows)
                                                        {
                                                            if (@_dr["Isalloted"].ToString() == "1")
                                                            {

                                                                <tr class="success">
                                                                    <td>@_dr["RowNo"].ToString()</td>
                                                                    <td>@_dr["Name"].ToString()</td>
                                                                    <td>@_dr["Studentid"].ToString()</td>
                                                                    <td>@_dr["SequenceNumber"].ToString()</td>
                                                                    <td>@_dr["InstituteName"].ToString() - ( @_dr["Institute_Id"].ToString())</td>
                                                                    <td>@_dr["ProgramLevel"].ToString()</td>
                                                                    <td>@_dr["Discipline"].ToString()</td>
                                                                    <td>@_dr["NoC"].ToString()</td>
                                                                    <td>@_dr["BranchName"].ToString()</td>
                                                                    <td>@_dr["SeatWaivertype"].ToString()</td>
                                                                    <td>@_dr["Remarks"].ToString()</td>
                                                                    <td>@_dr["EligibilityCriteria"].ToString()</td>
                                                                    <td>@_dr["Q1Qualification"].ToString() - @_dr["Q1Percentage"].ToString()%  </td>
                                                                    <td>@_dr["JEEMainReq"].ToString()  @_dr["JEEMainscoreReq"].ToString()   </td>
                                                                    <td>@_dr["JEEAdvanceReq"].ToString()  @_dr["JEEAdvanceScoreReq"].ToString()   </td>
                                                                    <td>@_dr["IELTSReq"].ToString()  @_dr["IELTSscoreReq"].ToString()   </td>
                                                                    <td>@_dr["GMATReq"].ToString()  @_dr["GMATScoreReq"].ToString()   </td>
                                                                    <td>@_dr["TOFELReq"].ToString()  @_dr["TOFELScoreReq"].ToString()   </td>
                                                                    <td>@_dr["SATReq"].ToString()  @_dr["SATScoreReq"].ToString()   </td>
                                                                </tr>
                                                            }

                                                            else
                                                            {
                                                                <tr>
                                                                    <td>@_dr["RowNo"].ToString()</td>
                                                                    <td>@_dr["Name"].ToString()</td>
                                                                    <td>@_dr["Studentid"].ToString()</td>
                                                                    <td>@_dr["SequenceNumber"].ToString()</td>
                                                                    <td>@_dr["InstituteName"].ToString() - ( @_dr["Institute_Id"].ToString() )</td>
                                                                    <td>@_dr["ProgramLevel"].ToString()</td>
                                                                    <td>@_dr["Discipline"].ToString()</td>
                                                                    <td>@_dr["NoC"].ToString()</td>
                                                                    <td>@_dr["BranchName"].ToString()</td>
                                                                    <td>@_dr["SeatWaivertype"].ToString()</td>
                                                                    <td>@_dr["Remarks"].ToString()</td>
                                                                    <td>@_dr["EligibilityCriteria"].ToString()</td>
                                                                    <td>@_dr["Q1Qualification"].ToString() - @_dr["Q1Percentage"].ToString()%  </td>
                                                                    <td>@_dr["JEEMainReq"].ToString()  @_dr["JEEMainscoreReq"].ToString()   </td>
                                                                    <td>@_dr["JEEAdvanceReq"].ToString()  @_dr["JEEAdvanceScoreReq"].ToString()   </td>
                                                                    <td>@_dr["IELTSReq"].ToString()  @_dr["IELTSscoreReq"].ToString()   </td>
                                                                    <td>@_dr["GMATReq"].ToString()  @_dr["GMATScoreReq"].ToString()   </td>
                                                                    <td>@_dr["TOFELReq"].ToString()  @_dr["TOFELScoreReq"].ToString()   </td>
                                                                    <td>@_dr["SATReq"].ToString()  @_dr["SATScoreReq"].ToString()   </td>
                                                                </tr>}
                                                        }
                                                      
                                                    }
                                                    else
                                                    {
                                                        <tr>
                                                            <td colspan="6">
                                                                No data found.
                                                            </td>
                                                        </tr>
                                                    }
                                                }
                                            }

                                        </tbody>
                                    </table>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section styles{
    <link href="~/assets/lib/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/assets/lib/datatables/css/dataTables.bootstrap.min.css" />
    <link href="~/assets/lib/datatables/css/dataTables.bootstrap.min.css" rel="stylesheet" />


}
@section scripts{
    <script src="/assets/lib/jquery/jquery.min.js" type="text/javascript"></script>
    <script src="/assets/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>
    <script src="/assets/js/main.js" type="text/javascript"></script>
    <script src="/assets/lib/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/js/jquery.dataTables.min.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/js/dataTables.bootstrap.min.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/dataTables.buttons.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.html5.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.flash.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.print.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.colVis.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.bootstrap.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#mainToggle").addClass("be-wrapper be-collapsible-sidebar be-collapsible-sidebar-collapsed");

            $("#tbl1").DataTable({
                searching: true,
                paging: true,
                info: true,                   // TO DISPLAY THE INFO 'SHOWING 1 TO X OF Y ENTRIES'
                lengthMenu: [[25, 50, -1], [25, 50, "All"]],
                dom: 'Bfrtip',
                buttons: [
            {
                extend: 'csvHtml5',
                text: 'Export',
                titleAttr: 'CSV'
            },
                ],
                columnDefs: [{
                    "targets": 5,
                    "orderable": false
                }]
            });

        });
    </script>
}





@{
    ViewBag.Title = "Course";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<style>
    p {
        margin: 0 0 0px !important;
    }

    .bgpartitionA {
        background-color: #82E0AA !important;
    }

    .bgpartitionB {
        background-color: #F7DC6F !important;
    }

    .bgpartitionC {
        background-color: #F0B27A !important;
    }

    .center {
        text-align: center !important;
    }
</style>
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default panel-border-color panel-border-color-custom panel-table">
            <div class="panel-heading">
                Mock Result - 1st Round
                <div class="tools">
                    <a href="@Url.Content("~/")Admin/MockRound" class="btn btn-success">Back</a>
                </div>
            </div>
            <div class="tab-container">
                <div class="tab-content">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="panel-body table-responsive">
                                <table class="table table-bordered table-hover" id="tbl" style="overflow:auto;">
                                    <thead>
                                        <tr>
                                            <th>Sr</th>
                                            <th>Student Id</th>
                                            <th>Student Name</th>
                                            <th>Country</th>
                                            <th>Merit Marks</th>
                                            <th>Merit Rank</th>
                                            <th>Program Level</th>
                                            <th>Discipline</th>
                                            <th>Allotted Institute</th>
                                            <th>Allotted Course</th>
                                            <th>Allotted Branch</th>
                                            <th>Fees Wavier</th>
                                            <th>Choice Number</th>
                                            <th>Eligibiity Criteria</th>

                                            <th>Skype Interview</th>
                                            <th>Skype Interview Complete</th>
                                            <th>Seat Approve</th>
                                            <th>Seat Approve Date</th>
                                            <th>Remarks</th>

                                        </tr>
                                    </thead>
                                    <tbody id="tbodyCostofliving">
                                        @{
                                            SIIModel.Admin.MockRound _obj = new SIIModel.Admin.MockRound();
                                            SIIRepository.Adminservice.ReportsRepository _objRepository = new SIIRepository.Adminservice.ReportsRepository();
                                            _obj.Type = "Count";
                                            System.Data.DataSet _ds = _objRepository.Report("ViewAllottedDetails_First", "ViewAllottedDetails_First");
                                            if (_ds != null)
                                            {
                                                if (_ds.Tables[0].Rows.Count > 0)
                                                {
                                                    foreach (System.Data.DataRow _dr in _ds.Tables[0].Rows)
                                                    {
                                                        <tr>
                                                            <td>@_dr["RowNo"].ToString()</td>
                                                            <td>@_dr["Student_ID"].ToString()</td>
                                                            <td> @_dr["FirstName"].ToString() @_dr["MiddleName"].ToString() @_dr["LastName"].ToString()</td>
                                                            <td>@_dr["country_name"].ToString()</td>
                                                            <td>@_dr["Merit_marks"].ToString()</td>
                                                            <td>@_dr["Merit_No"].ToString()</td>
                                                            <td>@_dr["ProgramLevel"].ToString()</td>
                                                            <td>@_dr["Discipline"].ToString()</td>
                                                            <td>@_dr["institutename"].ToString()   </td>
                                                            <td>@_dr["Natureofcourse"].ToString()  </td>
                                                            <td> @_dr["BranchName"].ToString()  </td>
                                                            <td> @_dr["AllocateSeatIn"].ToString()  </td>
                                                            <td> @_dr["SequenceNumber"].ToString()  </td>
                                                            <td> @_dr["EligibilityCriteria"].ToString()  </td>

                                                            <td>@_dr["IsRequireSkypeInterview"].ToString()</td>
                                                            <td>@_dr["IsSkypeInterviewComplete"].ToString()</td>
                                                            <td>@_dr["SeatApprove"].ToString()</td>
                                                            <td>@_dr["SeatApproveDate"].ToString()</td>
                                                            <td>@_dr["SeatApproveReason"].ToString()</td>
                                                        </tr>
                                                    }
                                                }
                                                else
                                                {
                                                    <tr>
                                                        <td colspan="6">
                                                            No data found.
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                            else
                                            {
                                                <tr>
                                                    <td colspan="6">
                                                        No data found.
                                                    </td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section styles{
    <link rel="stylesheet" type="text/css" href="/assets/lib/datatables/css/dataTables.bootstrap.min.css" />
    <link rel="stylesheet" href="/assets/css/style.css" type="text/css" />
}
@section scripts{
    <script src="/assets/lib/jquery/jquery.min.js" type="text/javascript"></script>
    <script src="/assets/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>
    <script src="/assets/js/main.js" type="text/javascript"></script>
    <script src="/assets/lib/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/js/jquery.dataTables.min.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/js/dataTables.bootstrap.min.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/dataTables.buttons.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.html5.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.flash.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.print.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.colVis.js" type="text/javascript"></script>
    <script src="/assets/lib/datatables/plugins/buttons/js/buttons.bootstrap.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#mainToggle").addClass("be-wrapper be-collapsible-sidebar be-collapsible-sidebar-collapsed");
            //$('#tbl').DataTable({
            //    dom: 'Bfrtip',
            //    buttons: ['excel']
            //});

            $("#tbl").DataTable({
                searching: true,
                paging: true,
                info: true,                   // TO DISPLAY THE INFO 'SHOWING 1 TO X OF Y ENTRIES'
                lengthMenu: [[25, 50, -1], [25, 50, "All"]],
                dom: 'Bfrtip',
                buttons: [
            {
                extend: 'csvHtml5',
                text: 'Export',
                titleAttr: 'CSV'
            },
                ],
                columnDefs: [{
                    "targets": 5,
                    "orderable": false
                }]
            });


        });
    </script>
}


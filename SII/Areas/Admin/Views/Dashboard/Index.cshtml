    <style>
        .badge {
            font-size: 12px;
        }

        .panel-full-color .panel-heading {
            color: #fff;
            margin: 0;
            padding: 15px 20px;
        }
        .pstyle {
            /*position: absolute;
            left: 250px;
            top: 00px;*/
            text-align: center;
        }
    </style>
}
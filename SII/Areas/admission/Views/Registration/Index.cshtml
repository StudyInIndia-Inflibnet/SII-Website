<body class="be-splash-screen">
    <div class="be-wrapper be-login be-registration">
        <div class="be-content">
            <input type="hidden" id="hdfBaseUrl" value="@Url.Content("~/")" />
            <div class="main-content container-fluid">
                <div class="sign-up">
                    <div class="panel panel-default panel-border-color panel-border-color-primary">
                        <div class="panel-heading">
                            <div class="col-md-4"><a href="@Url.Content("~/")home"> <img src="~/img/logo.png" class="logo-img"></a></div>
                            <div class="col-md-4"><center><span class="splash-title xs-pt-20"><strong>Student Registration</strong></span></center></div>
                            <div class="col-md-4"><img src="~/img/mhrd.png" class="img-responsive pull-right" /></div>
                        </div>
                        <div class="clearfix"></div>
                        <div class="panel-body xs-pt-30">
                            <form id="frmRegistration">
                                <div class="form-group xs-pt-10 col-md-3">
                                    <label>First Name<span style="color: red"> *</span></label>
                                    <input type="text" required placeholder="Enter First Name" name="FirstName" class="form-control allow_only_alphabet">
                                </div>
                                <div class="form-group xs-pt-10 col-md-3">
                                    <label>Last name<span style="color: red"> *</span></label>
                                    <input type="text" required placeholder="Enter Last Name" name="LastName" class="form-control allow_only_alphabet">
                                </div>
                                <div class="form-group xs-pt-10 col-md-6">
                                    <label>Mobile<span style="color: red"> *(This will be used only for contact purpose)</span></label>
                                    <input type="text" required placeholder="Enter Mobile" name="Mobile" maxlength="12" data-parsley-type="number" class="form-control allow_only_number" id="txtmobile">
                                </div>
                                <div class="form-group xs-pt-10 col-md-3">
                                    <label>Country<span style="color: red"> *</span></label>
                                    @Html.DropDownList("Country", new SelectList(ViewBag.Country, "Country_id", "Country_Name"),
                                    "--Select--",
                                    new
                                    {
                                        @class = "form-control",
                                        @id = "drpCountryofStay",
                                        @required = ""
                                    })
                                </div>
                                <div class="form-group xs-pt-10 col-md-3">
                                    <label>Date of Birth<span style="color: red"> *</span></label>
                                    <input type="text" id="txtdateofbirth" class="form-control datetimepicker" required name="DateOfBirth" placeholder="Date of Birth" readonly>
                                    <input type="text" id="txtYear" class="form-control" style="display:none">
                                </div>
                                <div class="form-group xs-pt-10 col-md-6">
                                    <label>Email<span style="color: red"> *</span></label>
                                    <input type="email" required placeholder="Enter email" name="Email" class="form-control" id="txtemailreg">
                                </div>
                                <div class="form-group xs-pt-10 col-md-6">
                                    <label>Password<span style="color: red"> *</span></label>
                                    <input type="password" required placeholder="Enter Password" class="form-control" id="txtpassword" name="ActualPassword">
                                </div>
                                <div class="form-group xs-pt-10 col-md-6">
                                    <label>Confirm Password<span style="color: red"> *</span></label>
                                    <input type="password" required placeholder="Enter Confirm Password" name="Email" class="form-control" id="txtconfirmpassord" data-parsley-equalto="#txtpassword" data-parsley-error-message="Confirm password should be same as password.">
                                </div>


                                <div class="form-group xs-pt-10 col-md-6">
                                    <label for="exampleInputEmail1" style="display:none">Captcha:<span style="color: red"> *</span></label><br />
                                    @Html.reCaptcha()
                                    @*<div class="g-recaptcha" data-sitekey="6LcvdFgUAAAAADND8gfV1qlBhPhrXHkueioemwjB"></div>*@
                                    @*<img id="imgLoginCaptcha" src="/Captcha/GetCaptchaImage" tabindex="-1">
                                        <a href="javascript:;" tabindex="-1" class="btn btn-warning" id="btnRefreshCaptcha"><i class="fa fa-refresh"></i>Refresh</a>*@
                                </div>
                                @*<div class="form-group xs-pt-10 col-md-6">
                                        <label for="exampleInputEmail1">Enter Captcha:<span style="color: red"> *</span></label><br />
                                        <input class="form-control" type="text" required="" autocomplete="off" placeholder="Enter Captcha" name="CaptchaStr" id="txtCaptcha">
                                    </div>*@
                                <div class="form-group xs-pt-10 col-md-6">
                                    <button type="submit" class="btn btn-block btn-primary btn-xl pull-right" id="btnRegister">Sign Up</button>
                                </div>
                                @*<div class="form-group xs-pt-10 col-md-3 pull-right">
                                        <a href="@Url.Content("~/")admission/login" class="btn btn-block btn-success btn-xl">Learner Login</a>
                                    </div>*@
                            </form>
                        </div>
                    </div>
                </div>
                <div class="splash-footer"><span>Already have an account? <a href="@Url.Content("~/")admission/login">Login</a></span></div>
            </div>
        </div>
    </div>


@{
    ViewBag.Title = "Submitted";
    Layout = "~/Views/Shared/_LayoutportalChoiceFilling.cshtml";
}

<h2>Submitted</h2>
<div class="row">
    <div class="loading"></div>
    <div class="col-md-12">
        <a href="~/admission/Dashboard" class="btn btn-sm btn-info pull-right"><i class="fa fa-arrow-left"></i> Home</a>
    </div>
    <div class="col-md-12">
        <div class="f1-steps">
            <div class="f1-progress">
                <div class="f1-progress-line" data-now-value="100" data-number-of-steps="6" style="width: 100%;"></div>
            </div>
            @Html.Partial("_ProgressBar")
        </div>
    </div>
    <hr />
    <fieldset class="col-md-12">
        <div class="panel panel-default ">
            <div class="panel-body">
                <h3 class="text-center">
                    Thank You.<br />
                    Your application has been submitted.
                </h3>
                <hr />
                <a href="~/admission/Dashboard" class="btn btn-warning">Back to Dashboard</a>
            </div>
        </div>
    </fieldset>
</div>
@section styles{
    <link href="~/assets/lib/wizard/style.css" rel="stylesheet" />
}
@section scripts{
    <script src="~/assets/lib/wizard/retina-1.1.0.min.js"></script>
    <script src="~/assets/lib/wizard/scripts.js"></script>
    <script src="~/Scipts/Areas/admission/StudentChoiceFilling/Submit.js"></script>

}

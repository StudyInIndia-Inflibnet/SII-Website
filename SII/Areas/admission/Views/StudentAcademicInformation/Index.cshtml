
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layoutportal.cshtml";
}

@section PageHead{
    <div class="row">
        <div class="col-md-12">
            <h3> Academic Information&nbsp;<span style="color: red">( @Session["studentid"].ToString() -  @Session["studentname"].ToString() )</span></h3>
        </div>
    </div>
}
<div class="loading" style="display:none;"></div>
<input type="hidden" id="hdnfilesize" value="5242880" />
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default panel-border-color panel-border-color-primary border-login">
            <div class="tab-container">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="panel-body table-responsive">
                            <h3 class="page-header">Educational Qualification</h3>
                            <form id="frm">
                                @Html.AntiForgeryToken()
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="table-responsive">
                                            <table class="table table-bordered table-striped">
                                                <thead>
                                                    <tr>
                                                        <th style="width:13%"></th>
                                                        <th>Name of the Board / University</th>
                                                        <th style="width:10%">Year</th>
                                                        <th style="width:7%">Obtained Marks</th>
                                                        <th style="width:7%">Maximum Marks</th>
                                                        <th style="width:10%">Country</th>
                                                        <th>Address</th>
                                                        <th>Subjects Studied</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td>
                                                            <label>Secondary School / Class X / Equivalent Qualification&nbsp;<span style="color:red">*</span></label>
                                                        </td>
                                                        <td><input type="text" class="form-control allow_only_alphabet" data-parsley-pattern="/^[a-zA-Z][a-zA-Z ]+$/" data-parsley-pattern-message="Only alphabets allowed." required="" id="txtBUni_Matri" /></td>
                                                        <td><input type="text" class="form-control" required="" id="txtYear_Matri" minlength="4" maxlength="7" /></td>
                                                        <td><input type="text" class="form-control" required="" id="txtMarks_Matri" maxlength="5" /></td>
                                                        <td><input type="text" class="form-control" required="" id="txtMMarks_Matri" maxlength="5" /></td>
                                                        <td>
                                                            @Html.DropDownList("CountryToStay", new SelectList(ViewBag.Country, "Country_id", "Country_Name"),
                                                        "--Select--",
                                                        new
                                                        {
                                                            @class = "form-control",
                                                            @id = "drpCountry_Matri",
                                                            @required = ""
                                                        })
                                                        </td>
                                                        <td><textarea rows="2" id="txtaddress_Matri" required="" class="form-control"></textarea></td>
                                                        <td><input type="text" class="form-control" required="" id="txtSub_Matri" /></td>
                                                    </tr>

                                                    <tr>
                                                        <td colspan="2">
                                                            <label>
                                                                Grade Conversion Document(Official document to convert Grades/CGPA to Percentage Scale):
                                                            </label>
                                                        </td>
                                                        <td colspan="3">
                                                            <input type="file" id="fuGradeConversionDocument_Matri" />
                                                            <input type="hidden" id="hdfGradeConversionDocument_Matri" value="" />
                                                            <a class="btn btn-warning" id="btnUploadGradeConversionDocument_Matri">Upload Document</a>
                                                            <a class="btn btn-success" id="btnViewGradeConversionDocument_Matri" style="display:none;" target="_blank" href="">View Document</a>
                                                            @*<br />
                                    (Only PDF)*@
                                                        </td>
                                                        <td colspan="3">
                                                            <input type="text" class="form-control allow_only_decimal txtTotalMarksinPercentage_Matri" required="" id="txtTotalMarksinPercentage_Matri" max="100" placeholder="Total Marks in Percentage" />
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td colspan="8">
                                                            Higher Secondary / Class XII / Equivalent Qualification <input id="rbtresultawaited" type="radio" name="result" value="false" /> Results Awaited
                                                            &nbsp;&nbsp;<input id="rbtresultdeclared" type="radio" name="result" value="1" checked="checked" />  Results Declared
                                                            &nbsp;&nbsp;<input id="rbtnotapplicableresultdeclared" type="radio" name="result" value="false" style="display:none" />
                                                        </td>
                                                    </tr>
                                                    <tr class="trElevenIntermediate" style="display:none">
                                                        <td>
                                                            <label>Eleven Equivalent Qualification &nbsp;<span style="color:red">*</span></label>
                                                        </td>
                                                        <td><input type="text" class="form-control allow_only_alphabet" data-parsley-pattern="/^[a-zA-Z][a-zA-Z ]+$/" data-parsley-pattern-message="Only alphabets allowed." required="" id="txtBUni_Ele" /></td>
                                                        <td><input type="text" class="form-control" required="" id="txtYear_Ele" minlength="4" maxlength="7" /></td>
                                                        <td><input type="text" class="form-control" required="" id="txtMarks_Ele" maxlength="5" /></td>
                                                        <td><input type="text" class="form-control" required="" id="txtMMarks_Ele" maxlength="5" /></td>
                                                        <td>
                                                            @Html.DropDownList("CountryToStay", new SelectList(ViewBag.Country, "Country_id", "Country_Name"),
                                                    "--Select--",
                                                    new
                                                    {
                                                        @class = "form-control",
                                                        @id = "drpCountry_Ele",
                                                        @required = ""
                                                    })
                                                        </td>
                                                        <td><textarea rows="2" id="txtaddress_Ele" required="" class="form-control"></textarea></td>
                                                        <td><input type="text" class="form-control" id="txtSub_Ele" required="" /></td>
                                                    </tr>
                                                    <tr class="trElevenIntermediate" style="display:none">
                                                        <td colspan="2">
                                                            <label>
                                                                Grade Conversion Document(Official document to convert Grades/CGPA to Percentage Scale):
                                                            </label>
                                                        </td>
                                                        <td colspan="3">
                                                            <input type="file" id="fuGradeConversionDocument_Ele" />
                                                            <input type="hidden" id="hdfGradeConversionDocument_Ele" value="" />
                                                            <a class="btn btn-warning" id="btnUploadGradeConversionDocument_Ele">Upload PDF</a>
                                                            <a class="btn btn-success" id="btnViewGradeConversionDocument_Ele" style="display:none;" target="_blank" href="">View PDF</a>
                                                            @*<br />
                                    (Only PDF)*@
                                                        </td>
                                                        <td colspan="3">
                                                            <input type="text" class="form-control allow_only_decimal txtTotalMarksinPercentage_Ele" max="100" required="" id="txtTotalMarksinPercentage_Ele" placeholder="Total Marks in Percentage" />
                                                        </td>
                                                    </tr>
                                                    <tr class="trHigherSecondaryIntermediate">
                                                        <td>
                                                            <label>Higher Secondary / Class XII / Equivalent Qualification &nbsp;<span style="color:red">*</span></label>
                                                        </td>

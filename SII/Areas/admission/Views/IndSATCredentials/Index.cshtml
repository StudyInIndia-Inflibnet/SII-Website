@using SIIRepository.StudentRegService
@using System.Data
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- FAVICON -->
    <link rel="icon" href="~/img/favicon.ico">
    <title>Study in India</title>
    <link rel="stylesheet" type="text/css" href="~/assets/lib/perfect-scrollbar/css/perfect-scrollbar.min.css" />
    <link rel="stylesheet" type="text/css" href="~/assets/lib/material-design-icons/css/material-design-iconic-font.min.css" />
    <link href="~/css/fonts/MaterialDesign-Webfont-master/css/materialdesignicons.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/assets/css/style.css" type="text/css" />
    <link href="~/assets/lib/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <link href="~/css/custom-institute.css" rel="stylesheet" />
    <style>
        .jconfirm-box {
            width: 90% !important;
            max-width: 400px;
        }
    </style>
    @Html.Partial("_GoogleTagManagerHead")
    <script src="~/assets/lib/jquery/jquery.min.js" type="text/javascript"></script>
</head>

<body>
    <input type="hidden" value="@Url.Content("~/")" id="hdfBaseUrl" />
    <div class="be-wrapper be-collapsible-sidebar be-collapsible-sidebar-collapsed">
        @{
            DataSet _ds = (new StudentRepository()).SELECT_IndSAT_Credential(Session["studentid"].ToString());
            DataRow _dr = _ds.Tables[0].Rows[0];
        }
        <div class="be-content">
            <div class="main-content container-fluid">
                <div class="row" id="">
                    <div class="col-md-12">
                        <table class="table table-borderless">
                            <tr>
                                <td class="text-left col-md-4"><img src="~/img/IndSAT/MHRD.png" width="250" /></td>
                                <td class="text-center col-md-4"><img src="~/img/IndSAT/SII.png" width="250" /> <h3>Ind-SAT 2020</h3></td>
                                <td class="text-right col-md-4"><img src="~/img/IndSAT/NTA.png" width="350" /></td>
                            </tr>
                        </table>
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <td>
                                        <table class="table table-bordered">
                                            <thead>
                                                <tr>
                                                    <td class="col-md-4">Firstname: <strong>@_dr["FirstName"].ToString()</strong></td>
                                                    <td class="col-md-4">Middlename:  <strong>@_dr["FatherName"].ToString()</strong></td>
                                                    <td class="col-md-4">Lastname:  <strong>@_dr["LastName"].ToString()</strong></td>
                                                </tr>
                                                <tr>
                                                    <td>Date of Birth:  <strong>@Convert.ToDateTime(_dr["DateOfBirth"].ToString()).ToString("dd-MM-yyyy")</strong></td>
                                                    <td>Gender:  <strong>@_dr["Gender"].ToString()</strong></td>
                                                    <td>Email:  <strong>@_dr["Email"].ToString()</strong></td>
                                                </tr>
                                                <tr>
                                                    <td>Contact Number:  <strong>@_dr["Mobile"].ToString()</strong></td>
                                                    <td>Country (Nationality):  <strong>@_dr["Country_Name"].ToString()</strong></td>
                                                    <td>SII ID: <strong>@_dr["studentid"].ToString()</strong></td>
                                                </tr>
                                            </thead>
                                        </table>
                                    </td>
                                </tr>
                            </thead>
                        </table>
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <td>
                                        <table class="table table-bordered">
                                            <thead>
                                                <tr>
                                                    <td class="col-md-4">Course Applied For</td>
                                                    <td class="col-md-4"><strong>@ViewBag.PL</strong></td>
                                                    <td class="col-md-4 text-center" rowspan="6">
                                                        <strong class="col-md-12">Candidate Photo</strong>
                                                        <img src="data:image/jpeg;base64,@Session["Photo"].ToString()" style="width: 200px;border: 1px solid;box-shadow: 5px 5px 7px rgba(0,0,0,0.3);" id="imgPhoto" />
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>Reporting Time (local time)</td>
                                                    <td><strong>@ViewBag.ReportingTime</strong></td>
                                                </tr>
                                                <tr>
                                                    <td>Examination Start Time (local time)</td>
                                                    <td><strong>@ViewBag.ExaminationTime</strong></td>
                                                </tr>
                                                <tr>
                                                    <td>Duration of Examination</td>
                                                    <td><strong>90 Minutes</strong></td>
                                                </tr>
                                                <tr>
                                                    <td>Date of Examination</td>
                                                    <td><strong>16<sup>th</sup> May 2020</strong></td>
                                                </tr>
                                                <tr>
                                                    <td>Login ID</td>
                                                    <td><strong>@_dr["studentid"].ToString()</strong></td>
                                                </tr>
                                                <tr>
                                                    <td class="col-md-4" rowspan="2"></td>
                                                    <td class="col-md-4" rowspan="2"></td>
                                                    <td class="col-md-4 text-center">
                                                        <strong class="col-md-12">Candidate Singnature</strong>
                                                        <img src="data:image/jpeg;base64,@Session["Signature"].ToString()" style="width: 200px; border: 1px solid;box-shadow: 5px 5px 7px rgba(0,0,0,0.3);" id="imgPhoto" />
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td class="text-center"><strong>Senior Director - NTA</strong></td>
                                                </tr>
                                            </thead>
                                        </table>
                                    </td>
                                </tr>
                            </thead>
                        </table>
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <td>
                                        <h4 class="text-danger text-center" style="font-weight:600;">IMPORTANT INSTRUCTIONS FOR CANDIDATES</h4>
                                        <ol>
                                            <li>Candidate is strongly advised to access the examination link 30 minutes ahead of time and download the launcher application.</li>
                                            <li>Candidate needs to ensure facing camera all the time during the exam,  the candidate cannot take a break in between the exam.</li>
                                            <li>Candidate is advised to take the assessment on a laptop or desktop.</li>
                                            <li>Candidate needs to ensure, they have good and consistent internet connectivity.</li>
                                            <li>Candidate is advised not to toggle between the assessment console and other application else your system will get locked.</li>
                                            <li>Do not press F5 to refresh the assessment console during the examination else your system will get locked.</li>
                                            <li>Candidate needs to ensure no other person comes in front of your camera else your exam will be cancelled.</li>
                                            <li>No Candidate should adopt any unfair means, or indulge in any unfair examination practices.  Use of any electronic device or calculator or rough sheet is prohibited.</li>
                                            <li>The Candidature of Candidates in case of impersonation will be automatically cancelled without any notice.</li>
                                            <li>Candidate needs to ensure they click Submit button at the end of the assessment.</li>
                                        </ol>
                                        <p class="text-center" style="margin:0;">
                                            <strong>For Any Assistance, Please contact:</strong>
                                        </p>
                                        <p class="text-center text-info" style="margin:0;">Toll free no: +91 9899-450-350</p>
                                        <p class="text-center text-info" style="margin:0;">Whatsapp: +91 8130-013-265</p>
                                        <p class="text-center text-info" style="margin:0;">Email ID: help.studyinindia@gov.in</p>
                                    </td>
                                </tr>
                            </thead>
                        </table>
                    </div>
                    <div class="col-md-12 text-center">
                        @*<button class="btn btn-warning" onclick="window.location.href='@Url.Content("~/")admission/IndSATCredentials/GeneratePDF'">Print</button>*@
                        <button class="btn btn-warning " id="btn-print" onclick="window.print();">Print</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <style>
        .be-collapsible-sidebar-collapsed .be-content {
            margin: 0;
        }
        .be-wrapper {
            min-height: 100%;
            padding-top: 0;
        }
        @@page {
            size: A4;
            margin: 0;
        }

        @@media print {
            html, body {
                width: 210mm;
                height: 297mm;
            }
            #btn-print{
                display:none;
            }
            /* ... the rest of the rules ... */
        }
    </style>
    <script src="~/assets/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>
    <script src="~/assets/js/main.js" type="text/javascript"></script>
    <script src="~/assets/lib/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/assets/lib/sweetalert2/sweetalert2.min.js"></script>
    @Html.Partial("_GoogleTagManagerBody")
    <script type="text/javascript">
        $(document).ready(function () {

        });
    </script>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-118738693-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', 'UA-118738693-1');
    </script>
</body>
</html>
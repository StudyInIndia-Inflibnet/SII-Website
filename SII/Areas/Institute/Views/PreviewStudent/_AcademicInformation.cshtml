<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        @{
            SIIRepository.StudentRegService.DashboardRepository _objRepository = new SIIRepository.StudentRegService.DashboardRepository();
            System.Data.DataSet _ds = _objRepository.Get_Dashboard_Modal_Data(TempData.Peek("studentid").ToString(), "Academic");
            if (_ds != null)
            {
                <h2 class="text-center">Education Qualifications</h2>
                if (_ds.Tables[0].Rows.Count > 0)
                {
                    var index = 1;
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>srno</th>
                                <th>Education Qualifications Name</th>
                                <th>EQName</th>
                                <th>Obtained Marks</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (System.Data.DataRow _dr in _ds.Tables[0].Rows)
                            {

                                <tr>
                                    <td>@(index++)</td>
                                    <td>@_dr["EduQualificationsName"].ToString()</td>
                                    <td>@_dr["EQName"].ToString()</td>
                                    <td>@_dr["EC_Perc"].ToString()</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                }
                else
                {
                    <table class="table table-bordered">
                        <tbody>
                            <tr>
                                <td>No data available.</td>
                            </tr>
                        </tbody>
                    </table>
                }

                <h2 class="text-center">Additional Qualifications</h2>
                if (_ds.Tables[1].Rows.Count > 0)
                {
                    var index = 1;
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>srno</th>
                                <th>Additional Qualifications Name</th>
                                <th>Score</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (System.Data.DataRow _dr in _ds.Tables[1].Rows)
                            {
                             <tr>
                                 <td>@(index++)</td>
                                 <td>@_dr["AEName"].ToString()</td>
                                 <td>@_dr["Score"].ToString()</td>
                             </tr>
                            }
                        </tbody>
                    </table>
                }
                else
                {
                    <table class="table table-bordered">
                        <tbody>
                            <tr>
                                <td>No data available.</td>
                            </tr>
                        </tbody>
                    </table>
                }
            }
        }
    </div>
</div>
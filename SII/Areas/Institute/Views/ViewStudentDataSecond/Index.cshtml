<div class="row" style="display:block" id="content">
    <div class="col-md-12">
        <div class="panel panel-default panel-border-color panel-border-color-primary border-login">
            <div class="tab-container">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="panel-body table-responsive">
                            @{
                                SIIRepository.StudentRegService.DashboardRepository _objRepository = new SIIRepository.StudentRegService.DashboardRepository();
                                System.Data.DataSet _ds = _objRepository.Get_Dashboard_Modal_Data(ViewBag.studentid, "Doc");
                                if (_ds != null)
                                {
                                    <h3 class="page-header">Document Information</h3>
                                    if (_ds.Tables[1].Rows.Count > 0)
                                    {
                                        int indexInternationalCollaboration = 1;
                                        <div class="col-md-6">
                                            <h4 class="page-header">Document Information</h4>
                                            <table class="table table-condensed table-responsive table-striped">
                                                <thead>
                                                    <tr>
                                                        <th>Sr no</th>
                                                        <th>Document Name</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody id="tbodydocument">

                                                    @foreach (System.Data.DataRow _dr in _ds.Tables[1].Rows)
                                                    {
                                                        <tr>
                                                            <td>
                                                                @(indexInternationalCollaboration++)
                                                            </td>
                                                            <td>@_dr["document_name"].ToString()</td>
                                                            <td><a href="@_dr["document_path"].ToString()" class="btn btn-warning" target="_blank">View</a></td>
                                                        </tr>
                                                    }
                                                </tbody>

                                            </table>

                                        </div>
                                    }
                                    if (_ds.Tables[0].Rows.Count > 0)
                                    {
                                        System.Data.DataRow _dr = _ds.Tables[0].Rows[0];
                                        <div class="col-md-6">
                                            <h4 class="page-header">Profile Picture</h4>
                                            @if (_dr["student_path"].ToString() != "")
                                            {
                                                <img class="img-rounded xs-mr-10" id="imgProfile" src="@_dr["student_path"].ToString()" width="140" height="140" />
                                            }
                                            else
                                            {
                                                <img class="img-rounded xs-mr-10" id="imgProfile" src="~/assets/img/140x140.png" width="140" height="140" />
                                            }
                                        </div>
                                    }
                                }
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row" style="display:block">
    <div class="col-md-12">
        <div class="panel panel-default panel-border-color panel-border-color-custom">
            <div class="tab-container">
                <div class="tab-content">
                    <h3>Accept / Reject</h3>
                    <form id="frmAcceptdeny" method="post">
                        <div class="row" id="divskypeinterview">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Skype Interview </label>
                                    <select class="form-control" name="IsRequireSkypeInterview" id="drpskypeinterview" required>
                                        <option selected="selected" value="REQUIRE">Yes</option>
                                        <option value="NOT-REQUIRE">No</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row" style="display:none" id="divSkypeInterviewCompleted">
                            <div class="col-md-6">
                                <div class="form-group">

                                    <select class="form-control" name="IsSkypeInterviewComplete" id="drpSkypeInterviewCompleted">
                                        <option value="COMPLETE">Yes</option>
                                        <option selected="selected" default value="PENDING">No</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row" style="display:none" id="divAcceptReject">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Accept / Reject&nbsp;<span style="color:red">*</span></label>
                                    <select class="form-control" name="SeatApprove" required id="drpacceptdeny">
                                        <option value="ACCEPT">Accept</option>
                                        <option value="REJECT">Reject</option>
                                    </select>
                                </div>
                            </div>
                        </div>

                        <div class="row" style="display:none" id="divStudentJoinNotJoin">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Student Admission Status&nbsp;<span style="color:red"></span></label>
                                    <select class="form-control" name="IsStudentAcceptSeat" id="drpStudentJoinNotJoined">
                                        <option value="JOINED">Joined</option>
                                        <option value="NOT-JOINED">Not Joined</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row" style="display:none" id="divRemarks">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Remarks&nbsp;<span style="color:red">*</span></label>
                                    <textarea id="textremarks" rows="3" name="SeatApproveReason" required="" class="form-control"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="row" id="divBtnSave">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-7">
                                            <button id="btnSave" type="button" class="btn btn-primary">Save</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
